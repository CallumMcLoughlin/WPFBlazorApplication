@using LibraryTest
@page "/"

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.


<h1>Result: @currentHeading</h1>

<p>
    <input type="number" @bind="A"></input>
    <input type="number" @bind="B"></input>
    
    <button @onclick="UpdateHeading">
        Update heading
    </button>
</p>

@code {
    private int A { get; set; } = 5;
    private int B { get; set; } = 7;

    private string currentHeading = "12";

    private void UpdateHeading()
    {
        currentHeading = new NumberClass().AddTwoNumbers(A, B).ToString();
    }
}